{
  "compilerOptions": {
    "jsx": "react",
    "jsxFragmentFactory": "Fragment",
    "noEmit": true,
    "skipLibCheck": true,
    "baseUrl": ".",
    "module": "commonjs",
    "target": "es2018",
    "sourceMap": true,
    "removeComments": true,
    "strict": true,
    "noImplicitAny": false,
    "strictNullChecks": false,
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "noImplicitReturns": true,
    "noFallthroughCasesInSwitch": true,
    "moduleResolution": "node",
    "esModuleInterop": true,
    "resolveJsonModule": true,
    "types": ["node"]
  }
}
